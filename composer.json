{
    "name": "proconsultancy/ats",
    "description": "Modern PHP ATS with MVC Architecture",
    "type": "project",
    "require": {
        "php": "^8.1",
        "vlucas/phpdotenv": "^5.6",
        "openai-php/client": "^0.10.1",
        "smalot/pdfparser": "^2.11",
        "symfony/var-dumper": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "authors": [
        {
            "name": "ProConsultancy Team",
            "email": "dev@proconsultancy.com"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
